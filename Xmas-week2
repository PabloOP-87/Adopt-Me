-- hello
local function checkExecutorStrength()

    local success = pcall(function()
        setreadonly(string, false)
        string.test = function() end
        setreadonly(string, true)
    end)
    
    return success
end

if checkExecutorStrength() then
    print("✓ strong executor, loading normal version...")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/PabloOP-87/Adopt-Me/refs/heads/main/Xmas-2025"))()
else
    print("✗ weak executor, loading lite version...")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/PabloOP-87/Adopt-Me/refs/heads/main/Xmas-2025-Lite"))()
end
